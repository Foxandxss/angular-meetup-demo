{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "noUnusedLocals": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-example/shared/product/data-access/types": [
        "libs/shared/product/data-access/types/src/index.ts"
      ],
      "@nx-example/shared/product/data-access/mock-data": [
        "libs/shared/product/data-access/mock-data/src/index.ts"
      ],
      "@nx-example/shared/product/data-access/mock-data/testing": [
        "libs/shared/product/data-access/mock-data/src/testing.ts"
      ],
      "@nx-example/shared/product/ui": ["libs/shared/product/ui/src/index.ts"],
      "@nx-example/products/feature-home-page": [
        "libs/products/feature-home-page/src/index.ts"
      ],
      "@nx-example/cart/feature-cart-page": ["libs/cart/feature-cart-page/src/index.ts"],
      "@nx-example/shared/util-e2e": ["libs/shared/util-e2e/src/index.ts"],
      "@nx-example/shared/util-jsxify": ["libs/shared/util-jsxify/src/index.ts"],
      "@nx-example/shared/cart/data-access-state": ["libs/shared/cart/data-access-state/src/index.ts"],
      "@nx-example/shared/product/data-access/state": [
        "libs/shared/product/data-access/state/src/index.ts"
      ],
      "@nx-example/shared/ui-header": ["libs/shared/ui-header/src/index.ts"],
      "@nx-example/products/feature-product-detail-page": [
        "libs/products/feature-product-detail-page/src/index.ts"
      ],
      "@nx-example/shared/styles": ["libs/shared/styles/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
